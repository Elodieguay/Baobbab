{
  "name": "baobbab",
  "version": "1.0.0",
  "main": "index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r --no-bail build",
    "dev": "pnpm --parallel --filter './apps/**' dev",
    "dev:deps": "pnpm --parallel --filter './packages/**' dev",
    "lint": "pnpm eslint",
    "lint:fix": "pnpm eslint --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "typescript-eslint": "^8.16.0"
  }
}
